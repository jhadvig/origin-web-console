{
  "name": "kubernetes-label-selector",
  "version": "2.1.1",
  "description": "Provides a LabelSelector object that understand kubernetes labels and label selector syntax, and works directly with JSON API objects from kubernetes.  Also provides a label filtering widget.",
  "moduleType": [
    "globals"
  ],
  "keywords": [
    "kubernetes",
    "angularjs",
    "labels"
  ],
  "authors": [
    "Jessica Forrester <jforrest@redhat.com>"
  ],
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "main": [
    "labelSelector.js",
    "labelFilter.js",
    "labelFilter.css"
  ],
  "dependencies": {
    "angular": ">=1.3.8 <1.6",
    "selectize": "~0.12.1",
    "fontawesome": "~4.3.0",
    "lodash": "~4.17.4"
  },
  "devDependencies": {
    "patternfly": ">=3.5.1 <4.0"
  }
}
